#!/bin/bash

if [ ! -f "$1" ]; then
	echo "No: $1"
	exit 1
fi

gs -dSAFER -dBATCH -dNOPAUSE -dQUIET -sDEVICE=epsonc -r180 -sOutputFile=- $1 | \
	lpr -P TallyGenicom_5040

